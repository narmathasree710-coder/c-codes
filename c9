
#include <stdio.h>
#include <string.h>
int main() 
{
    char n[10],a;
    int i,len;
    printf("Enter your mobile Number: ");
    scanf("%s",&n);
    len=strlen(n);
    if(len!=10){
        printf("number must have 10 digits");
        return 0;
    }
    if(n[0]=='0'){
        printf("number should not start with 0");
        return 0;}
        
    for(i=0;i<len;i++){
        a=n[i];
    if(a<'0'||a>'9')
    {
        printf("Invalid");}
    }
    printf("Valid Mobile Number");
    return 0;
}
